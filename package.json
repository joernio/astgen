    "@types/n-readlines": "^1.0.6",
    "@types/node": "^18.11.9",
    "@types/yargs": "^17.0.33",
    "esbuild": "0.25.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4"
