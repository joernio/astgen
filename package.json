{
  "name": "@joernio/astgen",
  "version": "3.23.0",
  "description": "Generate JS/TS AST in json format with Babel",
  "exports": "./index.js",
  "keywords": [
    "AST",
    "Static Analysis"
  ],
  "bin": {
    "astgen": "./dist/astgen.js"
  },
  "repository": "https://github.com/joernio/astgen",
  "author": "Max Leuthaeuser",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.24.7",
    "n-readlines": "^1.0.1",
    "readdirp": "^4.0.2",
    "yargs": "^17.7.2",
    "typescript": "^5.5.3"
  },
  "scripts": {
    "test": "jest --silent --config=\" jest.config.js\"",
    "build": "tsc --build",
    "binary": "pkg . --options max-old-space-size=8192 --no-bytecode --no-native-build --public --compress GZip --targets node18-linux-x64,node18-linux-arm64,node18-macos-x64,node18-win-x64,node18-macos-arm64"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "./dist/**/*.js"
  ],
  "directories": {
    "test": "./test/"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/n-readlines": "^1.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.2.4",
    "@types/yargs": "^17.0.33",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "ts-jest": "^29.2.5"
  }
}
