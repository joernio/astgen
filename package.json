    "yargs": "^17.7.2"
  },
  "scripts": {
    "test": "jest --silent --config=jest.config.js",
    "build": "tsc --build",
    "bundle": "esbuild ./dist/astgen.js --bundle --platform=node --outfile=./bundle/astgen.js"
