{
  "name": "@joernio/astgen",
  "version": "3.24.0",
  "description": "Generate JS/TS AST in json format with Babel",
  "exports": "./index.js",
  "keywords": [
    "AST",
    "Static Analysis"
  ],
  "bin": {
    "astgen": "./dist/astgen.js"
  },
  "repository": "https://github.com/joernio/astgen",
  "author": "Max Leuthaeuser",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.24.7",
    "n-readlines": "^1.0.1",
    "readdirp": "^4.0.2",
    "yargs": "^17.7.2",
    "typescript": "^5.5.3"
  },
  "scripts": {
    "test": "jest --silent --config=\" jest.config.js\"",
    "build": "tsc --build",
    "build_win": "bun build --compile --target=bun-windows-x64 --minify --sourcemap --bytecode ./dist/astgen.js --outfile astgen-win-x64.exe",
    "build_linux": "bun build --compile --target=bun-linux-x64 --minify --sourcemap --bytecode ./dist/astgen.js --outfile astgen-linux-x64",
    "build_linux_arm": "bun build --compile --target=bun-linux-arm64 --minify --sourcemap --bytecode ./dist/astgen.js --outfile astgen-linux-arm64",
    "build_macos": "bun build --compile --target=bun-darwin-x64 --minify --sourcemap --bytecode ./dist/astgen.js --outfile astgen-macos-x64",
    "build_macos_arm": "bun build --compile --target=bun-darwin-arm64 --minify --sourcemap --bytecode ./dist/astgen.js --outfile astgen-macos-arm64",
    "binary": "yarn build_win && yarn build_linux && yarn build_linux_arm && yarn build_macos && yarn build_macos_arm"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "./dist/**/*.js"
  ],
  "directories": {
    "test": "./test/"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/n-readlines": "^1.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.2.4",
    "@types/yargs": "^17.0.33",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "bun": "^1.2.4",
    "ts-jest": "^29.2.5"
  }
}
