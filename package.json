{
  "name": "@joernio/astgen",
  "version": "3.30.0",
  "description": "Generate JS/TS AST in json format with Babel",
  "exports": "./index.js",
  "keywords": [
    "AST",
    "Static Analysis"
  ],
  "bin": {
    "astgen": "./dist/astgen.js"
  },
  "repository": "https://github.com/joernio/astgen",
  "author": "Max Leuthaeuser",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.27.3",
    "n-readlines": "^1.0.1",
    "readdirp": "^4.1.2",
